<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:javaee="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WhiteLab" version="3.0">
  <servlet>
    <servlet-name>whitelab</servlet-name>
    <servlet-class>com.uvt.whitelab.WhiteLab</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>whitelab</servlet-name>
    <url-pattern>/page/*</url-pattern>
    <url-pattern>/explore/*</url-pattern>
    <url-pattern>/search/*</url-pattern>
  </servlet-mapping>
  <session-config>
    <tracking-mode>COOKIE</tracking-mode>
  </session-config>
